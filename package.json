  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pygy/morgul-road.git"
  },
  "keywords": [
    "SPA",
    "router"
  ],
  "author": "Leo Horie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pygy/morgul-road/issues"
  },
  "homepage": "https://github.com/pygy/morgul-road#readme",
  "devDependencies": {
    "eslint": "^2.2.0",
    "in-publish": "^2.0.0",
    "rollup": "^0.25.2",
    "uglify-js": "^2.6.0"
  }

}
